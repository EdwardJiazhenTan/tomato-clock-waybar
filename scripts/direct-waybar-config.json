{
    "custom/tomato": {
        "exec": "cat ~/.config/tomato-clock/waybar-output.json",
        "return-type": "json",
        "interval": 1,
        "on-click": "~/.config/waybar/scripts/simple-toggle.sh",
        "on-click-middle": "~/.local/bin/tomato-clock stop",
        "on-click-right": "~/.local/bin/tomato-clock skip"
    }
} 